{
  "name": "selenium-grid",
  "version": "1.0.0",
  "description": "Testing web sites with WebdriverIO and Selenium Grid",
  "scripts": {
    "check": "npm run lint && npm run test",
    "lint": "eslint \"test/**/*.ts\"",
    "lint:fix": "eslint \"test/**/*.ts\" --fix",
    "format": "prettier --check \"test/**/*\"",
    "format:fix": "prettier --write \"test/**/*\"",
    "test": "wdio run test/wdio.conf.ts",
    "test:watch": "watch \"wdio run test/wdio.conf.debug.ts\" test --wait=1",
    "test:grid": "wdio run test/wdio.conf.grid.ts",
    "clean": "rimraf build/"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@wdio/cli": "^7.19.0",
    "@wdio/jasmine-framework": "^7.19.0",
    "@wdio/local-runner": "^7.19.0",
    "@wdio/spec-reporter": "^7.19.0",
    "chromedriver": "^99.0.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "geckodriver": "^3.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "watch": "^1.0.2",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-geckodriver-service": "^2.1.0",
    "webdriverio": "^7.19.1"
  },
  "engines": {
    "node": ">=14.18.1",
    "npm": ">=6.14.15"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
